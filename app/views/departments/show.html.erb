<dl class="dl-horizontal lage-dt">
	<% done_list = {}%>
	<% @department.must.each {|m| %>
	<%= render('show', :m => m) if done_list[m.id].nil?%>
	<% done_list[m.id] = true }%>

	<% @department.may.each {|m| %>
	<%= render('show', :m => m) if done_list[m.id].nil?%>
	<% done_list[m.id] = true }%>
</dl>


